{
  "name": "gk",
  "version": "0.0.1",
  "description": "A simple CLI wrapper for adding components from gauthamkrishna.dev component registry using shadcn.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start:dev": "node dist/index.js",
    "publish:release": "pnpm build && pnpm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gk",
    "cli",
    "shadcn",
    "components",
    "registry",
    "heyitsgautham"
  ],
  "author": "Gautham Krishna <heyitsgautham@gmail.com>",
  "license": "MIT",
  "homepage": "https://gauthamkrishna.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heyitsgautham/portfolio.git",
    "directory": "packages/gk"
  },
  "bugs": {
    "url": "https://github.com/heyitsgautham/portfolio/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  },
  "packageManager": "pnpm@10.10.0"
}